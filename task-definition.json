{
  "family": "sample-fargate", 
  "executionRoleArn": "arn:aws:iam::287445272123:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
      {
          "name": "wtq", 
          "image": "httpd:2.4", 
          "portMappings": [
              {
                  "containerPort": 80, 
                  "hostPort": 80, 
                  "protocol": "tcp"
              }
          ], 
          "essential": true, 
          "entryPoint": [
              "sh",
              "-c"
          ], 
          "command": [
              "/bin/sh -c node server.js"
          ]
      }
  ], 
  "requiresCompatibilities": [
      "FARGATE"
  ], 
  "cpu": "256", 
  "memory": "512"
}
